<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="takeiteasy.GUI.Controller.LocalLobbyCtrl">
   <children>
      <Label layoutX="102.0" layoutY="66.0" text="Players List">
         <font>
            <Font size="23.0" />
         </font>
      </Label>
      <Label layoutX="99.0" layoutY="314.0" text="Add New Player:" />
      <Button fx:id="submit" disable="true" layoutX="413.0" layoutY="311.0" mnemonicParsing="false" onAction="#addNewPlayer" prefHeight="25.0" prefWidth="80.0" text="Submit" />
      <TextField fx:id="nameField" layoutX="221.0" layoutY="311.0" prefHeight="25.0" prefWidth="180.0" />
      <ListView fx:id="playersListView" layoutX="102.0" layoutY="99.0" prefHeight="191.0" prefWidth="300.0" />
      <Button fx:id="remove" disable="true" layoutX="413.0" layoutY="101.0" mnemonicParsing="false" onAction="#removePlayer" prefHeight="25.0" prefWidth="80.0" text="Remove" />
      <Button fx:id="rename" disable="true" layoutX="413.0" layoutY="186.0" mnemonicParsing="false" onAction="#renamePlayer" prefHeight="25.0" prefWidth="80.0" text="Rename" />
      <TextField fx:id="renameField" layoutX="412.0" layoutY="239.0" prefHeight="24.0" prefWidth="138.0" visible="false" />
      <Button fx:id="confirmButton" disable="true" layoutX="481.0" layoutY="269.0" mnemonicParsing="false" onAction="#confirmRename" text="Confirm" visible="false" />
      <Button fx:id="cancel" layoutX="412.0" layoutY="269.0" mnemonicParsing="false" onAction="#cancelRename" text="Cancel" visible="false" />
      <Button fx:id="back" layoutX="162.0" layoutY="364.0" mnemonicParsing="false" onAction="#backToMainMenu" text="Main Menu" />
      <Button fx:id="start" disable="true" layoutX="365.0" layoutY="364.0" mnemonicParsing="false" onAction="#startMatch" text="Start Match" />
      <Button fx:id="setSeed" layoutX="541.0" layoutY="17.0" mnemonicParsing="false" onAction="#setSeed" text="Set" />
      <TextField fx:id="seedField" layoutX="358.0" layoutY="17.0" />
      <Label layoutX="310.0" layoutY="21.0" text="Seed" />
      <Label fx:id="seedLabel" layoutX="77.0" layoutY="22.0" text="Not Set" />
      <Label layoutX="25.0" layoutY="21.0" text="Seed:">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Line endX="556.5" layoutX="65.0" layoutY="354.0" startX="-65.0" />
      <Line endX="535.0" layoutX="85.0" layoutY="56.0" startX="-85.0" />
   </children>
